<% include partials/header %>
	<style>
		fieldset.account {
			width: 512px;
			margin: 0 auto;
			padding: 32px 0;
			border: 2px solid rgb(238, 238, 238);
			}
			fieldset.account + fieldset.account {
				margin-top: 64px;
				}
			fieldset.account legend {
				margin-left: 16px;
				padding: 0 16px;
				font-size: 16px;
				}

		#invites {
			list-style: none;
			}
			#invites li {
				height: 36px;
				margin: 0 32px;
				padding: 0 8px;
				background-color: rgb(238, 238, 238);
				line-height: 36px;
				}
				#invites li + li {
					margin-top: 32px;
					}
				#invites span.fa {
					height: 36px;
					margin: 0 8px 0 0;
					display: inline-block;
					float: right;
					line-height: 36px;
					cursor: pointer;
					}


				#invites li.used {
					opacity: 0.5;
					text-decoration: line-through;
					}
	</style>
	<main>
		<h1 class="title slab">Account</h1>
		<fieldset>
			<legend>Settings</legend>
			<ul>
				<li></li>
			</ul>
		</fieldset>
	</main>
<% include partials/footer %>
